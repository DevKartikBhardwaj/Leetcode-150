class Solution {
    public boolean isAnagram(String s, String t) {
        if(s.length()!=t.length())
            return false;
        
        int[] hash=new int[256];

        for(char ch: s.toCharArray()){
           hash[ch]++;
        }

        for(char ch:t.toCharArray()){
            if(hash[ch]==0)
                return false;
            hash[ch]--;
        }

        for(int i=0;i<hash.length;i++){
            if(hash[i]>0)
                return false;
        }

        return true;
    }
}
